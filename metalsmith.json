{
  "source": "./content",
  "destination": "./build",
  "metadata": {
    "title": "Emma & Chris' Engagement",
    "description": "I asked, she agreed.",
    "bootstrap-version": "3.3.6"
  },
  "plugins": {
    "metalsmith-markdown": {
      "gfm": true,
      "tables": true,
      "pedantic": false,
      "sanitize": false,
      "smartLists": true,
      "smartypants": true
    },
    "metalsmith-layouts": {
      "engine": "mustache",
      "directory": "layouts",
      "partials": "partials",
      "default": "default.html",
      "pattern": "*.html"
    },
    "metalsmith-sass": {
      "sourceMap": true,
      "outputDir": "css/",
      "outputStyle": "compact"
    },
    "metalsmith-s3": {
  }
}
